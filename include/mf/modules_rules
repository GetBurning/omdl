#!/usr/bin/make -f
################################################################################
#
# Modules
#
################################################################################

################################################################################
# Rules
################################################################################

#------------------------------------------------------------------------------#
# Build Targets
#------------------------------------------------------------------------------#

.PHONY:	all-modules
all-modules: | silent
	$(call target_end)

#------------------------------------------------------------------------------#
# Clean Targets
#------------------------------------------------------------------------------#

.PHONY: clean-modules
clean-modules: | silent
	$(call target_end)

#------------------------------------------------------------------------------#
# Install / Uninstall Targets
#------------------------------------------------------------------------------#

.PHONY: install-modules
install-modules: | silent
	$(call target_end)

.PHONY: uninstall-modules
uninstall-modules: | silent
	$(call target_end)

#------------------------------------------------------------------------------#
# Information Targets
#------------------------------------------------------------------------------#

.PHONY: info-modules
info-modules: | silent
	$(call heading1, Project Modules)
	$(call enumerate_variable,library,$(false),$(false))
	$(call enumerate_variable,library_backup_add,$(false),$(false))
	$(call enumerate_variable,library_modules,$(false),$(false))

################################################################################
# eof
################################################################################
